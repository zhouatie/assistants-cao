#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhoushitie/My/codes/ts-cao/node_modules/.pnpm/ts-jest@29.3.2_4zwyxyqceen3vpk243mjufjbve/node_modules/ts-jest/node_modules:/Users/zhoushitie/My/codes/ts-cao/node_modules/.pnpm/ts-jest@29.3.2_4zwyxyqceen3vpk243mjufjbve/node_modules:/Users/zhoushitie/My/codes/ts-cao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhoushitie/My/codes/ts-cao/node_modules/.pnpm/ts-jest@29.3.2_4zwyxyqceen3vpk243mjufjbve/node_modules/ts-jest/node_modules:/Users/zhoushitie/My/codes/ts-cao/node_modules/.pnpm/ts-jest@29.3.2_4zwyxyqceen3vpk243mjufjbve/node_modules:/Users/zhoushitie/My/codes/ts-cao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
